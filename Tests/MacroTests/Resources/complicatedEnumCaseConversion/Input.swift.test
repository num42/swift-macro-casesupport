@CaseConversion
public enum ElementType: Codable, Equatable {
    case checkbox(checkboxElement: CheckboxElement)
    case dateEntry(dateEntryElement: DateEntryElement)
    case label(labelElement: LabelElement)
    case numberEntry(numberEntryElement: NumberEntryElement)
    case roomSelection(roomSelectionElement: RoomSelectionElement)
    case selectionList(selectionListElement: SelectionListElement)
    case signature(signatureElement: SignatureElement)
    case textEntry(textEntryElement: TextEntryElement)

    public var rawValue: String {
      String(String(describing: self).split(separator: "(")[0])
    }
}
